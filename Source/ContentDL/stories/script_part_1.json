    {
        "Left": "sanae_annoyed",
        "Right": "yoshika_sweating",
        "LeftMessage": "I'm not that, naive.",
        "RightMessage": ""
    },
    {
        "Left": "sanae_annoyed",
        "Right": "yoshika_sweating",
        "LeftMessage": "",
        "RightMessage": "Then it's useless ot argue."
    },
    {
        "Left": "sanae_annoyed",
        "Right": "yoshika_happy",
        "LeftMessage": "",
